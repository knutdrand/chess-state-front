{
  "name": "chess-state-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chrisoakman/chessboardjs": "",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.0",
    "@react-oauth/google": "^0.12.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chess.js": "^1.3.1",
    "expo-cli": "^6.3.10",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "",
    "react-bootstrap": "^2.10.10",
    "react-chessboard": "",
    "react-dom": "",
    "react-router-dom": "",
    "react-scripts": "",
    "web-vitals": ""
  },
  "scripts": {
    "generate-api": "npx openapi-typescript-codegen --input ./openapi.json --output ./src/api --client axios",
    "start": "npm run generate-api && react-scripts start",
    "build": "npm run generate-api && react-scripts build",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.25.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/utils/testUtils.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    }
  }
}
